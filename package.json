{
  "version": "2.2.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "watch": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "license-plate",
  "author": "Niels Bosman",
  "description": "Dutch license plate formatting and validation in JavaScript.",
  "repository": "git+https://github.com/niels-bosman/license-plate.git",
  "keywords": [
    "License plate",
    "Kenteken",
    "Formatting",
    "Formatteren",
    "Dutch",
    "Nederlands"
  ],
  "bugs": {
    "url": "https://github.com/niels-bosman/license-plate/issues"
  },
  "homepage": "https://github.com/niels-bosman/license-plate#readme",
  "module": "dist/license-plate.esm.js",
  "size-limit": [
    {
      "path": "dist/license-plate.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/license-plate.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.1.6",
    "size-limit": "11.1.6",
    "tsdx": "0.14.1",
    "tslib": "2.8.1",
    "typescript": "5.7.3"
  }
}
